{
  "Serilog": {
    "Using":  ["Serilog.Sinks.File"],
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      { "Name": "File", "Args": { "path": "./logs/qldv-.log", "rollingInterval": "Day"} }
    ]
  },
  "Minio": {
    "BaseUrl": "qldv-asset.local"
  }
}
